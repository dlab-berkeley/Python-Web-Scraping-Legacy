    "#List for tweets\n",
    "tweets = []\n",
    "\n",
    "#Open json file\n",
    "with open('tweet_data.json', 'r') as f:\n",
    "    #Iterate over file object\n",
    "    for line in f:\n",
    "        #convert each to a json dictionary\n",
    "        tweet = json.loads(line)\n",
    "        #append to tweets\n",
    "        tweets.append(tweet)"
